#PROJECT2
#DIAGNOSTIC NETWORK 

echo ======================= >> "hasil.txt"
echo network diagnostic >> "hasil.txt"
echo ======================= >> "hasil.txt"

echo --konfigurasi IP-- >> "hasil.txt"
ipconfig /all >> "hasil.txt"

echo --koneksi internet-- >> "hasil.txt"
ping 8.8.8.8 >> "hasil.txt"

echo --cek koneksi-- >> "hasil.txt"
ping google.com >> "hasil.txt"

echo --trace route-- >> "hasil.txt"
tracert google.com >> "hasil.txt"

echo --koneksi aktif-- >> "hasil.txt"
netstat -an >> "hasil.txt"

pause

#PROJECT3
#BACKUP AUTOMATION


@echo off
echo === Mulai Backup ===
mkdir "C:\Backup\$date$"
xcopy "D:\Myfiles\TK" "C:\Backup\$date$" /E /I /Y
echo === Verifikasi Folder ===
dir "C:\Backup|$date$"
pause

#PROJECT4
#HEALTHCEK

C:\Windows\system32> D:
D:\> mkdir Healthcek
D:\> systeminfo > D:\Healthcek\Systeminfo.txt
D:\> chkdsk > D:\Healthcek\Checkdisk.txy
D:\> tasklist > D:\Healthcek\Tasklist.txt

#PROJECT5
#CUSTOM CMD

echo @echo off > startup.bat
echo color 0A >> startup.bat
echo prompt $P$G >> startup.bat
.\startup.bat
